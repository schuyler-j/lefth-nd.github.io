  <h1 style="top: 220px">Welcome to My Website!</h1>
  <h2>I'm not a flag :(</h2>
  <h1 id="title" style="font-size: 32px; top: 320px">Find my flag!</h1>
  <button onClick="clickMe()" class="button">Click Me!</button>
  <h3>they tell me javascript is simple.js</h3>
  <script>
	  function clickMe(){
		  let count = window.getComputedStyle(document.getElementById("title"), null).getPropertyValue("font-size");
		  let title = document.getElementById("title");
		  count_ = parseFloat(count);
		  count_ = count_ + 5;
		  title.setAttribute("style", `font-size: ${count_}`+'px; top: 320px');
		  if (count_ > 80){
			  title.setAttribute("style", "display: none");
		  }
	  }
  </script>
